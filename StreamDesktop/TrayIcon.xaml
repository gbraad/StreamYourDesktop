<Window x:Class="StreamDesktop.TrayIcon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        xmlns:local="clr-namespace:StreamDesktop"
        mc:Ignorable="d"
        Title="TrayIcon" Height="300" Width="300">
    <Grid>
        <tb:TaskbarIcon ToolTipText="St(r)eam Desktop" x:Name="icon" TrayMouseDoubleClick="icon_TrayMouseDoubleClick" IconSource="Resources/stream.ico"/>
    </Grid>
</Window>
